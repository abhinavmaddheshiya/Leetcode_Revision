class Solution {
public:
    int maxSubarrays(vector<int>& nums) {
        int maxScore = INT_MAX;
        int n = nums.size();
        for(int i=0;i<n;i++)
            maxScore&=nums[i];
        if(maxScore != 0)
            return 1;
        int cnt=0;
        int curr = INT_MAX;
        for(int i=0;i<n;i++)
        {
            curr&=nums[i];
            if(curr == maxScore)
            {
                cnt++;
                curr = INT_MAX;
            }
        }
        return cnt;
    }
};
